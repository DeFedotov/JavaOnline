package by.epam.java.basic;

import java.util.Scanner;

public class Branching5 {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int x = sc.nextInt();

    if (x <= 3) {
      int y = x*x - 3*x + 9;
      System.out.println(y);
    } else {
      double y = 1 / (x*x*x + 6);
      System.out.println(y);
    }
  }
}
