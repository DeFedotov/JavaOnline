package by.epam.java.basic;

// Составить программу нахождения произведения квадратов первых двухсот чисел

public class Loop4 {
  public static void main(String[] args) {
    long prod = 1;
    for (int i = 1; i <= 9; i++) {
      long j = i * i;
      prod = prod * j;
    }
    System.out.println(prod);
  }
}
